{

	"manifest_version": 2,
	"name": "Line",
	"version": "0.2",
	"description": "Downloads LINE stickers.",

	"content_scripts": [
		{
			"matches": ["*://store.line.me/*"],
			"js": ["line.js"]
		}
	],

	"background": {
		"scripts": ["background.js"]
	},

	"browser_action": {

		"default_icon": {
			"16": "icons/page-16.png",
			"32": "icons/page-32.png"
		}
	}

}
